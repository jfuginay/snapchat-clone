{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.18.0",
      "cache": {
        "disabled": true
      },
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "@GOOGLE_SERVICE_INFO_PLIST"
      },
      "prebuildCommand": "scripts/eas-build-pre-install.sh"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "@GOOGLE_SERVICE_INFO_PLIST"
      },
      "prebuildCommand": "scripts/eas-build-pre-install.sh"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "@GOOGLE_SERVICE_INFO_PLIST"
      },
      "prebuildCommand": "scripts/eas-build-pre-install.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
